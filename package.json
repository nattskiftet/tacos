{
	"name": "tacos",
	"version": "0.0.1",
	"description": "Boostrap a Next.js project with components, API routes, and utilities.",
	"scripts": {
		"dev": "next dev",
		"lint": "xo --prettier",
		"fix": "xo --prettier --fix",
		"format": "prettier -w .",
		"format:check": "prettier --check .",
		"typescript:check": "tsc --noEmit --p tsconfig.json",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@heroicons/react": "^2.1.5",
		"@kuma-ui/core": "^1.5.7",
		"next": "^14.2.5",
		"react": "^18.3.1",
		"react-aria-components": "^1.3.0",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.6.1",
		"@kuma-ui/next-plugin": "^1.3.1",
		"@storybook/addon-essentials": "^8.2.5",
		"@storybook/addon-interactions": "^8.2.5",
		"@storybook/addon-links": "^8.2.5",
		"@storybook/addon-onboarding": "^8.2.5",
		"@storybook/blocks": "^8.2.5",
		"@storybook/html": "^8.2.5",
		"@storybook/nextjs": "^8.2.5",
		"@storybook/react": "^8.2.5",
		"@storybook/test": "^8.2.5",
		"@types/node": "20.14.12",
		"@types/react": "18.3.3",
		"eslint-config-xo-react": "^0.27.0",
		"prettier": "^3.3.3",
		"storybook": "^8.2.5",
		"typescript": "5.5.4",
		"xo": "^0.58.0"
	},
	"xo": {
		"extends": "eslint-config-xo-react",
		"ignores": [
			".next",
			"next-env.d.ts"
		],
		"overrides": [
			{
				"files": [
					"**/*.js"
				],
				"rules": {
					"unicorn/prefer-module": 0
				}
			}
		],
		"rules": {
			"import/extensions": 0,
			"react/boolean-prop-naming": [
				"error",
				{
					"rule": "^(is|has|can|should)[A-Z]([A-Za-z0-9]?)+"
				}
			]
		}
	}
}
